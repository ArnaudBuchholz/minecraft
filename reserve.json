{
    "port": 8080,
    "mappings": [{
      "method": "POST",
      "match": "^/rcon",
      "custom": "./rcon.js"
    }, {
      "method": "GET",
      "match": "^/data/buildings\\.json",
      "custom": "./buildings.js"
    }, {
      "method": "GET",
      "match": "^/data/(.*)",
      "file": "./data/$1"
    }, {
      "match": "^/(.*)",
      "file": "./www/$1"
    }]
  }